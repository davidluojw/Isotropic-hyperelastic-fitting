function res = loss(params, data, F, detF)

    % 提取 x 和 y 列数据
    x = data(:,2);
    y = data(:,3);

    F = {};
    detF = zeros(length(x), 1);
    for i = 1:length(x)
        F{i} = Tensor2_3D().gen_F(x(i), 1.0 / sqrt(x(i)), 1.0 / sqrt(x(i)));
        
        detF(i) = x(i) * 1.0 / sqrt(x(i)) * 1.0 / sqrt(x(i));  % incompressible, J= 1
    end

   
    P11 = PK1_stress(params, x, F, detF);


    
    % 计算残差
    res = P11 - y;
   
end